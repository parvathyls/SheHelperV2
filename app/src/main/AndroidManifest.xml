        </activity>
        <activity
            android:name=".RegisterParents"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".FetchRecords"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".SSReports"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AdmLandingPage"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ParentScreen"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AdmReports"
            android:label="RuralDevelopmentHackathonV1"
            android:theme="@style/AppTheme.NoActionBar">
